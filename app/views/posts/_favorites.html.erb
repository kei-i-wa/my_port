     <% if post.favorited_by?(current_user) %>
      <%= link_to post_favorites_path(post), method: :delete do %>
      <i class="fas fa-check"></i><%= post.favorites.count %><% end %>
      <% else %>
      <%= link_to post_favorites_path(post), method: :post do %>
      <i class="far fa-star"></i><%= post.favorites.count %><% end %>
      <% end %>
      
      <i class="far fa-comment-dots"><%=post.post_comments.count%></i>
      <%= attachment_image_tag post.user,:profile_image,size:"20x20",fallback: "no_image.png"%>
      <%= link_to user_path(post.user) do %><%=post.user.name%><% end %>
      <%=post.created_at.strftime('%Y/%m/%d') %>